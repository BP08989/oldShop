{% extends 'base.html.twig' %}

{% block body %}
    <h1>Attributevalue</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ attributeValue.id }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ attributeValue.value }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('attributevalue_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('attributevalue_edit', { 'id': attributeValue.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
